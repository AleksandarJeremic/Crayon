show create table prices:
CREATE TABLE `silver_prices` ( `enrollment_number` int(11) DEFAULT NULL, `meter_id` text CHARACTER SET utf8 COLLATE utf8_bin, `meter_name` text CHARACTER SET utf8 COLLATE utf8_bin, `meter_type` text CHARACTER SET utf8 COLLATE utf8_bin, `meter_category` text CHARACTER SET utf8 COLLATE utf8_bin, `meter_sub_category` text CHARACTER SET utf8 COLLATE utf8_bin, `service_family` text CHARACTER SET utf8 COLLATE utf8_bin, `product_name` text CHARACTER SET utf8 COLLATE utf8_bin, `sku_id` text CHARACTER SET utf8 COLLATE utf8_bin, `product_id` text CHARACTER SET utf8 COLLATE utf8_bin, `meter_region` text CHARACTER SET utf8 COLLATE utf8_bin, `unit_of_measure` text CHARACTER SET utf8 COLLATE utf8_bin, `part_number` text CHARACTER SET utf8 COLLATE utf8_bin, `effective_start_date` timestamp(6) NULL DEFAULT NULL, `effective_end_date` timestamp(6) NULL DEFAULT NULL, `unit_price` double DEFAULT NULL, `base_price` double DEFAULT NULL, `market_price` double DEFAULT NULL, `currency` text CHARACTER SET utf8 COLLATE utf8_bin, `included_quantity` double DEFAULT NULL, `offer_id` text CHARACTER SET utf8 COLLATE utf8_bin, `term` text CHARACTER SET utf8 COLLATE utf8_bin, `price_type` text CHARACTER SET utf8 COLLATE utf8_bin, SORT KEY `enrollment_number` (`enrollment_number`) , SHARD KEY () ) AUTOSTATS_CARDINALITY_MODE=INCREMENTAL AUTOSTATS_HISTOGRAM_MODE=CREATE AUTOSTATS_SAMPLING=ON SQL_MODE='ONLY_FULL_GROUP_BY,STRICT_ALL_TABLES'

show create table ca:
CREATE TABLE `silver_ca` ( `usage_start_time` date DEFAULT NULL, `billing_period_start` timestamp(6) NULL DEFAULT NULL, `billing_period_end` timestamp(6) NULL DEFAULT NULL, `effective_cost` double DEFAULT NULL, `unit_cost` double DEFAULT NULL, `usage_amount` double DEFAULT NULL, `consumed_unit` text CHARACTER SET utf8 COLLATE utf8_bin, `usage_unit` text CHARACTER SET utf8 COLLATE utf8_bin, `region` text CHARACTER SET utf8 COLLATE utf8_bin, `resource_group` text CHARACTER SET utf8 COLLATE utf8_bin, `resource_id` text CHARACTER SET utf8 COLLATE utf8_bin, `resource_name` text CHARACTER SET utf8 COLLATE utf8_bin, `availability_zone` text CHARACTER SET utf8 COLLATE utf8_bin, `subscription_id` text CHARACTER SET utf8 COLLATE utf8_bin, `sub_account_name` text CHARACTER SET utf8 COLLATE utf8_bin, `billing_account_name` text CHARACTER SET utf8 COLLATE utf8_bin, `charge_description` text CHARACTER SET utf8 COLLATE utf8_bin, `cost_type` text CHARACTER SET utf8 COLLATE utf8_bin, `charge_category` text CHARACTER SET utf8 COLLATE utf8_bin, `charge_frequency` text CHARACTER SET utf8 COLLATE utf8_bin, `pricing_model` text CHARACTER SET utf8 COLLATE utf8_bin, `pricing_category` text CHARACTER SET utf8 COLLATE utf8_bin NOT NULL, `tags` text CHARACTER SET utf8 COLLATE utf8_bin, `service_category` text CHARACTER SET utf8 COLLATE utf8_bin, `service_subcategory` text CHARACTER SET utf8 COLLATE utf8_bin, `service_name` text CHARACTER SET utf8 COLLATE utf8_bin, `service_family` text CHARACTER SET utf8 COLLATE utf8_bin, `publisher` text CHARACTER SET utf8 COLLATE utf8_bin, `currency` text CHARACTER SET utf8 COLLATE utf8_bin, `commitment_discount_id` text CHARACTER SET utf8 COLLATE utf8_bin, `commitment_discount_name` text CHARACTER SET utf8 COLLATE utf8_bin, `commitment_discount_category` text CHARACTER SET utf8 COLLATE utf8_bin, `commitment_discount_type` text CHARACTER SET utf8 COLLATE utf8_bin, `provider` text CHARACTER SET utf8 COLLATE utf8_bin NOT NULL, `invoice_issuer` text CHARACTER SET utf8 COLLATE utf8_bin NOT NULL, `sku_price_id` text CHARACTER SET utf8 COLLATE utf8_bin, SORT KEY `usage_start_time` (`usage_start_time`) , SHARD KEY () ) AUTOSTATS_CARDINALITY_MODE=INCREMENTAL AUTOSTATS_HISTOGRAM_MODE=CREATE AUTOSTATS_SAMPLING=ON SQL_MODE='ONLY_FULL_GROUP_BY,STRICT_ALL_TABLES'
